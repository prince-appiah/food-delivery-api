{
  "name": "food-delivery-api",
  "version": "1.0.0",
  "description": "Node.JS Restful CRUD API with Express and MySql",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "create:db": "sequelize db:create",
    "drop:db": "sequelize db:drop",
    "migrate:db": "sequelize db:migrate",
    "undo-migrate:db": "sequelize db:migrate:undo:all",
    "reset-migrate:db": "yarn undo-migrate:db & yarn migrate:db"
  },
  "dependencies": {
    "@sentry/node": "^6.9.0",
    "@sentry/tracing": "^6.9.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.5",
    "slugify": "^1.6.0",
    "validator": "^13.6.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  },
  "author": "Prince Appiah",
  "license": "ISC"
}
